            <Image Source="/CapaPresentacion;component/Image9.png" Stretch="Fill" Height="49.485" Width="240.794"/>
        </Menu>
        <TextBlock HorizontalAlignment="Left" Margin="293.644,27.482,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="24" Foreground="White" FontWeight="Bold" FontFamily="Arial Rounded MT Bold"><Run Text="MANTENIMIENTO"/><Run Text=" DE "/><Run Text="ADMINISTRACION"/><LineBreak/><Run/></TextBlock>
        <TextBox x:Name="txtCodigo" HorizontalAlignment="Left" Height="23" Margin="229.071,128.381,0,0" TextWrapping="Wrap" Text="CODIGO" VerticalAlignment="Top" Width="144.215" Background="White" Grid.IsSharedSizeScope="True" Foreground="Black" FontWeight="Bold" RenderTransformOrigin="0.5,0.5" SelectionBrush="Black" TextChanged="TextBox_TextChanged">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.458"/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <TextBox x:Name="txtDomicilio" HorizontalAlignment="Left" Height="23" Margin="230.081,219.187,0,0" TextWrapping="Wrap" Text="DOMICILIO" VerticalAlignment="Top" Width="144.215" Background="White" Grid.IsSharedSizeScope="True" Foreground="Black" FontWeight="Bold" RenderTransformOrigin="0.5,0.5" SelectionBrush="White">
