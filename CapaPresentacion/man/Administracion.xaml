                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.458"/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <DatePicker x:Name="dtaFechaNac" HorizontalAlignment="Left" Margin="228.06,270.641,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Foreground="Black"/>
        <Button x:Name="btmExistir" Content="EXISTIR" HorizontalAlignment="Left" Margin="693.197,186.681,0,0" VerticalAlignment="Top" Width="75" Click="btmExistir_Click"/>
        <Button x:Name="btmAsistir" Content="ASISTIR" HorizontalAlignment="Left" Margin="609.857,186.681,0,0" VerticalAlignment="Top" Width="75" Click="btmAsistir_Click"/>
        <Button x:Name="btmAgregar" Content="AGREGAR" HorizontalAlignment="Left" Margin="693,264,0,0" VerticalAlignment="Top" Width="75" Click="btmAgregar_Click"/>
        <Button x:Name="btmListar" Content="LISTAR" HorizontalAlignment="Left" Margin="645,305,0,0" VerticalAlignment="Top" Width="75" Click="btmListar_Click"/>
        <Button x:Name="btmEliminar" Content="ELIMINAR" HorizontalAlignment="Left" Margin="609.857,262.674,0,0" VerticalAlignment="Top" Width="75" Height="20.969" Click="btmEliminar_Click"/>
        <Button x:Name="btmActualizar1" Content="BUSCAR" HorizontalAlignment="Left" Margin="693.197,223.484,0,0" VerticalAlignment="Top" Width="75" Click="btmActualizar1_Click"/>
        <Button x:Name="btmActualizar" Content="ACTUALIZAR" HorizontalAlignment="Left" Margin="609.857,223.484,0,0" VerticalAlignment="Top" Width="75" Click="btmActualizar_Click"/>
        <Button x:Name="btmEscribir" Content="ESCRIBIR" HorizontalAlignment="Left" Margin="610,147,0,0" VerticalAlignment="Top" Width="75" Click="btmEscribir_Click"/>
        <Button x:Name="btmLeer" Content="LEER" HorizontalAlignment="Left" Margin="693,147,0,0" VerticalAlignment="Top" Width="75" Click="btmLeer_Click"/>
